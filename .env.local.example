# Mistral AI API Key
# Get your API key from https://console.mistral.ai/
MISTRAL_API_KEY=your_mistral_api_key_here

# NextAuth Configuration
# Generate AUTH_SECRET with: openssl rand -base64 32
NEXTAUTH_SECRET=your_generated_secret_here
AUTH_SECRET=your_generated_secret_here
NEXTAUTH_URL=http://localhost:3000

# Google OAuth Configuration
# Get your credentials from https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your_google_client_id_here.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Supabase Configuration
# Get your credentials from https://supabase.com/dashboard
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Flutterwave API Keys
# Get your keys from https://dashboard.flutterwave.com/dashboard/settings/apis
FLW_PUBLIC_KEY=FLWPUBK_TEST-your_public_key_here
FLW_SECRET_KEY=FLWSECK_TEST-your_secret_key_here
FLW_ENCRYPTION_KEY=FLWSECK_TEST-your_encryption_key_here
FLW_SECRET_HASH=your_webhook_secret_hash_here

# Vercel KV (Redis)
# Get your credentials from https://vercel.com/dashboard/stores
KV_URL=your_kv_url_here
KV_REST_API_URL=your_kv_rest_api_url_here
KV_REST_API_TOKEN=your_kv_rest_api_token_here
KV_REST_API_READ_ONLY_TOKEN=your_kv_read_only_token_here

# Admin Dashboard
# Set a secure password for the admin dashboard at /admin
ADMIN_PASSWORD=your_secure_admin_password_here

# Resend Email Service
# Get your API key from https://resend.com/api-keys
RESEND_API_KEY=re_your_resend_api_key_here

# Google Analytics 4
# Get your tracking ID from https://analytics.google.com/
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX

# Sentry Error Tracking
# Get your DSN from https://sentry.io/
NEXT_PUBLIC_SENTRY_DSN=https://your_sentry_dsn_here
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT=your_sentry_project
SENTRY_AUTH_TOKEN=your_sentry_auth_token

